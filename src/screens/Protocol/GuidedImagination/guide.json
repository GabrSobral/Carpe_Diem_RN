[
  { "id": "0", "text" : "" },
  { "id": "1", "text" : "Uma pequena meditação" },
  { "id": "2", "text" : "para promover a paz no seu interior." },
  { "id": "3", "text" : "Para começar," },
  { "id": "4", "text" : "procure consertar a sua postura," },
  { "id": "5", "text" : "encontrando a posição mais confortável" },
  { "id": "6", "text" : "Tranquilize-se," },
  { "id": "7", "text" : "escaneie mentalmente o seu corpo," },
  { "id": "8", "text" : "procurando identificar quaisquer áreas" },
  { "id": "9", "text" : "onde haja desconforto," },
  { "id": "10", "text" : "dor" },
  { "id": "11", "text" : "ou tensão," },
  { "id": "12", "text" : "agora, expire devagar," },
  { "id": "13", "text" : "até encher completamente os pulmões de ar," },
  { "id": "14", "text" : "faça uma pequena pausa" },
  { "id": "15", "text" : "e solte lentamente, produzindo um som abafado," }, 
  { "id": "16", "text" : "mas audível" },
  { "id": "17", "text" : "repetimos esse processo mais algumas vezes," },
  { "id": "18", "text" : "respire..." },
  { "id": "19", "text" : "uma pequena pausa..." },
  { "id": "20", "text" : "e agora solte o ar" },
  { "id": "21", "text" : "(realizar x3)" },
  { "id": "22", "text" : "A cada respiração," },
  { "id": "23", "text" : "imagine que o ar migra diretamente" },
  { "id": "24", "text" : "para as áreas de tensão," },
  { "id": "25", "text" : "refrigerando e amaciando a musculatura," },
  { "id": "26", "text" : "a cada expiração," },
  { "id": "27", "text" : "você visualiza toda a tensão" },
  { "id": "28", "text" : "fluindo para fora do seu corpo..." },
  { "id": "29", "text" : "Agora..." },
  { "id": "30", "text" : "Concentre-se em relaxar cada área do seu corpo," },
  { "id": "31", "text" : "da ponta dos pés," },
  { "id": "32", "text" : "até o topo da cabeça," },
  { "id": "33", "text" : "concentre-se principalmente" },
  { "id": "34", "text" : "na área dos ombros e pescoço," },
  { "id": "35", "text" : "se possível contraia os ombros" },
  { "id": "36", "text" : "junto com o pescoço o máximo que conseguir," },
  { "id": "37", "text" : "continue contraindo," },
  { "id": "38", "text" : "e em 3... 2… 1," },
  { "id": "39", "text" : "respire fundo," },
  { "id": "40", "text" : "e solte de uma vez," },
  { "id": "41", "text" : "isso mesmo," },
  { "id": "42", "text" : "contraia..." },
  { "id": "43", "text" : "sedure por 3... 2... 1..." },
  { "id": "44", "text" : "e solte novamente..." },
  { "id": "45", "text" : "muito bem." },
  { "id": "46", "text" : "A partir de agora" },
  { "id": "47", "text" : "permita que a sua respiração" },
  { "id": "48", "text" : "encontre o seu ritmo natural," },
  { "id": "49", "text" : "com os olhos fechados" },
  { "id": "50", "text" : "comece a se concentrar em você," },
  { "id": "51", "text" : "procure observar a sua mente" },
  { "id": "52", "text" : "de forma perceptiva..." },
  { "id": "53", "text" : "No que você está pensando neste momento?" },
  { "id": "54", "text" : "Existe algo em especial?" },
  { "id": "55", "text" : "Ou são apenas pensamentos variados?" },
  { "id": "56", "text" : "Não tente mudar nada," },
  { "id": "57", "text" : "apenas observe como quem" },
  { "id": "58", "text" : "observa as nuvens," },
  { "id": "59", "text" : "é importante que você não se apegue" },
  { "id": "60", "text" : "aos pensamentos," },
  { "id": "61", "text" : "assim como surgem, eles vão embora..." },
  { "id": "62", "text" : "Os pensamentos são como" },
  { "id": "63", "text" : "a sondas do mar," },
  { "id": "64", "text" : "eles vêm, e vão," },
  { "id": "65", "text" : "continuamente..." },
  { "id": "66", "text" : "e você se encontra dentro do mar" },
  { "id": "67", "text" : "de pensamentos," },
  { "id": "68", "text" : "até que em um certo momento," },
  { "id": "69", "text" : "você decide se levantar," },
  { "id": "70", "text" : "e sair da água," },
  { "id": "71", "text" : "então, você se senta na praia," },
  { "id": "72", "text" : "ali..." },
  { "id": "73", "text" : "você não se deixa levar pelas ondas," },
  { "id": "74", "text" : "você não é controlado por um" },
  { "id": "75", "text" : "pensamento especifico" },
  { "id": "76", "text" : "mas consegue contemplar" },
  { "id": "77", "text" : "o conjunto completo" },
  { "id": "78", "text" : "com uma postura de maturidade," },
  { "id": "79", "text" : "e consciência..." },
  { "id": "80", "text" : "A partir de então," },
  { "id": "81", "text" : "você vai voltando ao seu estado" },
  { "id": "82", "text" : "natural de atenção," },
  { "id": "83", "text" : "sem perder, no entando," },
  { "id": "84", "text" : "esse estado de serenidade e foco" },
  { "id": "85", "text" : "alcançados..." },
  { "id": "86", "text" : "Lembre-se, a mente não é uma máquina" },
  { "id": "87", "text" : "para trabalhar incansavelmente." },
  { "id": "88", "text" : "Fazer uma pausa, mesmo que pequena," },
  { "id": "89", "text" : "pode ajudar a encontrar equilibrio," },
  { "id": "90", "text" : "renovando as suas energias," },
  { "id": "91", "text" : "melhorando a sua produtividade" },
  { "id": "92", "text" : "e bem estar..." },
  { "id": "93", "text" : "Fique em paz..." },
